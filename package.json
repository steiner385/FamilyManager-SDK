{
  "name": "@familymanager/sdk",
  "version": "1.0.0",
  "description": "SDK for building FamilyManager plugins and extensions",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core/events": {
      "types": "./dist/core/events/index.d.ts",
      "import": "./dist/core/events/index.mjs",
      "require": "./dist/core/events/index.cjs"
    },
    "./core/events/EventBus": {
      "types": "./dist/core/events/EventBus.d.ts",
      "import": "./dist/core/events/EventBus.mjs",
      "require": "./dist/core/events/EventBus.cjs"
    },
    "./core/events/types": {
      "types": "./dist/core/events/types.d.ts",
      "import": "./dist/core/events/types.mjs",
      "require": "./dist/core/events/types.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build && tsc -p tsconfig.json --emitDeclarationOnly",
    "build:test": "tsc -p tsconfig.test.json --noEmit",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "storybook": "storybook dev -p 6010",
    "build-storybook": "storybook build --quiet",
    "test-storybook": "test-storybook --maxWorkers=2 --ci",
    "test-storybook:ci": "start-server-and-test 'serve storybook-static -p 6011 --no-request-logging' http://localhost:6011 'test-storybook --url http://localhost:6011'",
    "test-storybook:coverage": "test-storybook --coverage && serve-coverage",
    "test-storybook:watch": "concurrently \"serve storybook-static -p 6011 --no-request-logging\" \"test-storybook --watch --url http://localhost:6011\""
  },
  "keywords": [
    "familymanager",
    "sdk",
    "plugins"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@faker-js/faker": "^9.3.0",
    "@hono/node-server": "^1.13.7",
    "@playwright/experimental-ct-react": "^1.49.1",
    "@playwright/test": "^1.49.1",
    "@prisma/client": "^5.22.0",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/cli": "^7.6.7",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/types": "^7.6.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.19.70",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.0",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.16",
    "axe-playwright": "^2.0.3",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "node-fetch": "^2.7.0",
    "playwright": "^1.49.1",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.4",
    "serve-handler": "^6.1.6",
    "start-server-and-test": "^2.0.9",
    "storybook": "^7.6.7",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "vite": "^4.5.5",
    "wait-on": "^7.2.0",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.0",
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0"
  },
  "dependencies": {
    "hono": "^4.6.16",
    "jsonwebtoken": "^9.0.0",
    "pako": "^2.1.0",
    "tailwind-merge": "^2.6.0",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "rrule": "^2.7.2"
  }
}
